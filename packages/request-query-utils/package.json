{
  "name": "@sliit-foss/request-query-utils",
  "version": "0.0.0",
  "description": "A package to isolate filters and sorts from a given request's query parameters",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.js --bundle --platform=node --target=node14 --outfile=dist/index.js --sourcemap --minify",
    "bump-version": "pnpm build && npx automatic-versioning --name=@sliit-foss/request-query-utils",
    "release": "pnpm run bump-version && pnpm publish --access=public --tag=latest && git reset HEAD~1 || true",
    "test": "dotenv -- jest --coverage --verbose --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sliit-foss/npm-catalogue.git"
  },
  "homepage": "https://github.com/sliit-foss/npm-catalogue/packages/request-query-utils#readme",
  "keywords": [
    "query-utils",
    "filterquery",
    "sortquery",
    "filter",
    "sort"
  ],
  "author": "SLIIT FOSS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sliit-foss/npm-catalogue/issues"
  }
}